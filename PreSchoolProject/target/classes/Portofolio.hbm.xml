<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name = "com.www.preschool.dto.PortfolioDto" table = "portfolio">
      
      <meta attribute = "class-description">
         하이버네이트 포토폴리오 
      </meta>
      
      <id name = "no" type = "int" column = "no">
         <generator class="native"/>
      </id>
      
      <property name = "teacher_no" column = "teacher_no" type = "int"/>
      <property name = "child_no" column = "child_no" type = "int"/>
      <property name = "title" column = "title" type = "string"/>
      <property name = "comment" column = "comment" type = "string"/>
      <property name = "every_day" column = "every_day" type = "string"/>
      <property name = "photos_url" column = "photos_url" type = "string"/>
      
   </class>
</hibernate-mapping>